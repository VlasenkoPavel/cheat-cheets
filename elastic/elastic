{
  "script": {
    "source": "ctx._source.number += params.increment",
    "lang": "painless",
    "params": {
      "increment": 2
    }
  }
}

/// create index
PUT /cms
{
  "settings": {
    "number_of_shards": 5,
    "number_of_replicas": 1
  }
}

PUT /cms
{
  "settings": {
    "number_of_shards": 5,
    "number_of_replicas": 1
  },
  "mappings": {
    "properties": {
      "userId": {
        "type": "text"
      },
      "number": {
        "type": "integer"
      }
    }
  }
}

//update mapping
POST /cms/_mapping
{
  "properties": {
    "name": {
      "type": "text"
    }
  }
}



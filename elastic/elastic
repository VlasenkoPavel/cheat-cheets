curl -XGET -u elastic:123qwe -H "Content-Type: application/json" http://localhost:9200/ehr/_search -d '{"query": {"ids": {"values": ["7646ddc9-aa11-4047-84c9-2b0979757f8d","956c115b-6421-44db-80cd-e5bf12053cdd"]}}}' | jq


{
  "query": {
    "query_string": {
      "query": "(mkb10 OR mkb10~ OR mkb10*) AND (вак OR вак~ OR вак*)",
      "fields": ["id^2","name^2","title^2","value^1.5"],
      "default_operator": "AND",
      "fuzzy_prefix_length": 2
    }
  },
  "size": 100

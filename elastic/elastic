  "query": {
    "bool": {
      "should": [
        {
          "query_string": {
            "fields": ["title^2","value^1.5"],
            "query": "вакй*",
            "boost": 1
          }
        },
        {
          "query_string": {
            "fields": ["title^2","value^1.5"],
            "query": "вак~",
            "boost": 1,
            "fuzzy_prefix_length": 2
          }
        }
      ]
    }
  },
  "size": 100
}

